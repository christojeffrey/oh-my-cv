{
  "name": "@ohmycv/case-police",
  "version": "0.1.1",
  "description": "Use `case-police` in browser environment.",
  "homepage": "https://github.com/Renovamen/oh-my-cv/tree/main/packages/case-police",
  "bugs": {
    "url": "https://github.com/Renovamen/oh-my-cv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Renovamen/oh-my-cv.git",
    "directory": "packages/case-police"
  },
  "license": "MIT",
  "author": "Renovamen <renovamenzxh@gmail.com>",
  "files": [
    "dist",
    "dict"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build-fast:pkg": "tsup src/index.ts --format cjs,esm --no-dts && node -e \"const fs=require('fs');fs.mkdirSync('dist/dict',{recursive:true});['abbreviates','brands','general','products','softwares'].forEach(f=>fs.copyFileSync(`dict/${f}.json`,`dist/dict/${f}.json`))\"",
    "build:pkg": "tsup src/index.ts --format cjs,esm --dts && node -e \"const fs=require('fs');fs.mkdirSync('dist/dict',{recursive:true});['abbreviates','brands','general','products','softwares'].forEach(f=>fs.copyFileSync(`dict/${f}.json`,`dist/dict/${f}.json`))\""
  },
  "dependencies": {
    "case-police": "^0.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
